{
  "name": "pupil",
  "version": "0.0.1",
  "description": "Visualize project parameters from different sources.",
  "main": "pupil.js",
  "scripts": {
    "test": "npm run test-server && npm run test-ui",
    "test-server": "mocha --reporter spec test",
    "test-ui": "jest --config .jestrc",
    "coverage": "npm run coverage-server && npm run coverage-ui && npm run collect-coverage",
    "coverage-server": "istanbul cover --dir coverage/server ./node_modules/.bin/_mocha -- --reporter spec",
    "coverage-ui": "jest --config .jestrc-coverage",
    "collect-coverage": "./scripts/collect-coverage.js",
    "jshint": "jshint . --exclude node_modules",
    "jscs": "jscs .",
    "check": "npm run jshint && npm run jscs",
    "dev": "nodemon --config .nodemonrc ./pupil.js --no-ui & webpack-dev-server -d --content-base ui/ --inline --hot",
    "build": "webpack",
    "postinstall": "npm run build"
  },
  "keywords": [
    "git"
  ],
  "author": "Roman Decker <roman.decker@gmail.com>",
  "license": "ISC",
  "bin": "./pupil.js",
  "dependencies": {
    "arangojs": "^5.4.1",
    "babel-core": "^6.18.2",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "bluebird": "^3.4.6",
    "bulma": "^0.2.3",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.1",
    "d3": "^4.4.0",
    "event-to-promise": "^0.7.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "faker": "^3.1.0",
    "fs-extra-promise": "^0.4.1",
    "jest": "^17.0.3",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.2",
    "lorem-ipsum": "^1.0.3",
    "node-sass": "^3.13.1",
    "nodegit": "^0.16.0",
    "opn": "^4.0.2",
    "random-js": "^1.0.8",
    "rc": "^1.1.6",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^5.0.0",
    "redux": "^3.6.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "temp": "^0.8.3",
    "webpack": "^1.13.3",
    "wordwrap": "^1.0.0",
    "yargs": "^6.4.0"
  },
  "devDependencies": {
    "istanbul": "^0.4.5",
    "jscs": "^3.0.7",
    "jshint": "^2.9.4",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "webpack-dev-server": "^1.16.2"
  }
}
